@CHARSET "UTF-8";
body{
	background-color:#ebebeb;
	overflow:hidden;
	margin:0;
	}
#downloadBar {
	height: 57px;
	width: 100%;
	background-color: #ebebeb;
}
#winningImage {
	float: left;
	height: 57px;
	width: 147px;
}
#wnImage {
	height: 57px;
}
#downloadMiddle {
	height: 130px;
	width: 100%;
	background-color: #3ebde7;
	padding-top: 13%;
}
#signImage {
	width: 267px;
	margin: 0px auto;
}
#sgImage {
	width: 267px;
	margin-left:-4px;
}
#downLoad{
	width: 100px;
    height: 28px;
    background-color: #ffe3af;
    line-height:28px;
    color: #05607e;
    position: relative;
    margin-top: -23px;
    margin-left: 151px;
    border: 0.5px solid #0a85ae;
    border-radius: 5px;
}
#downLoad span{
	display:block;
	width:57px;
	font-size:14px;
	height:28px;
	margin:0px auto;
	line-height:30px;
}
#downLoadNotice{
	display:none;
}
#userLogin{
	display:none;
}
#certificate{
	display:none;
}
#doclist{
	display:none;
}
#record{
	display:none;
}
.noticeWord{
	color:#05607e;
	font-size:22px;
	font-weight:bold;
	margin: 20px;	
}
.noticeStep{
	color:#05607e;
	font-size:18px;
	margin:10px;
	margin-left:30px;
	font-weight:bold;	
}
.title{
	color:#05607e;
	font-size:18px;
	width:100px;
	height:30px;
	margin:0px auto;
	font-weight:bold;	
}
#loginTitle{
	width:75px;
}
#certificateTitle{
	width:166px;
}
#doclistTitle{
	width:200px;
}
#recordTitle{
	width:146px;
}
.word{
	margin:0px auto;
	width:310px;
	font-size:12px;
	font-weight:bold;
}
#imgFirst{
	width:102px;
	height:200px;
	margin:0px auto;
	margin-top: 10px;
}
#imgSecond{
	width:205px;
	height:200px;
	margin:0px auto;
	margin-top: 10px;
}
#imgThird{
	width:312px;
	height:200px;
	margin:0px auto;
	margin-top: 10px;
}
.img{
	width:100px;
	height:180px;
}
#copyRight{
	width:100%;
	height:40px;
	background-color:#ebebeb;
	color:#405a63;
	padding-top:30px;
	}
#wnCopyRight{
	width:155px;
	height:20px;
	font-size:14px;
	margin:0px auto;
    word-break: keep-all;
    white-space: nowrap;
	}
