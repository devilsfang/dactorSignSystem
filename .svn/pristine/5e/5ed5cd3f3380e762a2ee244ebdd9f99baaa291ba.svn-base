/* -----------------------------右边菜单栏样式-------------*/
#rightShow{
position: absolute;
right: 0px;
z-index: 2;
overflow-x:hidden; 
height: 80%; 
width: 110px; 
overflow-y:hidden;
bottom:0px;
}
#deTailsShow {
 height: 100%;  
width:55px; 
overflow: visible; 
border-right: 1px dashed #B7C9D2;;
box-shadow: 40px 8px 40px  rgba(231,240,243,0.5);
float:right;
margin-right: 50px;

bottom: 60px; 
display: block;
margin-top: 90px;
 position: relative; 
transition:0.5s;
 
}
.first {
height: 30px; 
width: 150px;
/* background-color: rgba(0,0,0,0.5);  */
margin-bottom: 15px;
border-radius: 50px 50px 50px 50px;

}
.details {
width: 75px;
color: white;
float: left; 
text-align: center;
font-size:13px;
line-height: 25px;
border-radius: 40px;
background-color: rgba(137,141,144,0.5);
}
.date {


float: left; 
font-size: 10px;
line-height:12px;
word-break: break-word;
 width: 30px;
}
.icon {
width: 80px; 
height: 60px;

bottom:0px; 
position: absolute;
right: 0px;
z-index: 99;
padding-bottom:20px;

}
#icon2 {

border-radius:50%;
width: 60px;
height: 60px;
background-color: rgba(93, 93, 93, 0.5);

 transition:0.5s;
}
#picture {
margin-top: 15px;
 margin-left: 15px;
}
