/* span{
float: left;
} */
body {
	overflow: visible;
	background: #fff;
}

#sliderContainer {
	height: 61px;
	width: 90%;
	background-color: #b9d1db;
	border-radius: 40px;
	border-top: solid 2px #999;
	position: absolute;
	left: 5%;
	bottom: 20px;
	z-index: 997;
}

#sliderBox {
	position: absolute;
	bottom: 20px;
	width: 90%;
	left: 5%;
	z-index: 998;
	opacity: 0.5; /*透明*/
	filter: Alpha(Opacitydd = 30); /*透明*/
	border-radius: 40px;
	overflow: hidden;
}

#slider {
	width: 57px;
	height: 57px;
	position: relative;
	margin: 0px auto;
	background-color: #FFF;
	border-radius: 50%;
	border-bottom: solid 2px;
	z-index: 999;
	bottom: 0px;
	opacity: 0.5; /*透明*/
	filter: Alpha(Opacitydd = 30); /*透明*/
}

#correct {
	font-size: 16px;
	height: 24px;
	width: 40px;
	float: left;
	color: #da0301;
	margin-top: 18px;
	margin-left: 20px;
}

#select {
	font-size: 12px;
	width: 114px;
	height: 20px;
	margin: 0px auto;
	position: relative;
	z-index: 1;
	margin-top: 20px;
	color: #e1e9eb;
}

#pass {
	font-size: 16px;
	height: 24px;
	width: 40px;
	float: right;
	color: #0378d8;
	margin-right: 20px;
	margin-top: 18px;
}

.arrow {
	height: 11px;
	width: 3opx;
}

@
keyframes pass {
	from {background: #b9d1db;
}

to {
	background: #0378d8;
}

}
@
keyframes correct {
	from {background: #b9d1db;
}

to {
	background: #da0301;
}

}
@
-webkit-keyframes pass /* Safari 与 Chrome */ {
	from {background: #b9d1db;
}

to {
	background: #0378d8;
}

}
@
-webkit-keyframes correct /* Safari 与 Chrome */ {
	from {background: #b9d1db;
}

to {
	background: #da0301;
}

}
.l-page-bottom-border:before {
	border-bottom: 0px solid #ccc;
}

#sliderContainerLay {
	width: 100%;
	overflow: hidden;
	background: #eee;
	display: block;
	position: absolute;
	bottom: 0px;
	height: 90px;
	z-index: 10;
}
/* fontSizeAdjust */
#fontSizeContainer{
	width:100%;
	height:160px;
	position:absolute;
	top:120px;
	z-index:1;
	overflow:hidden;
	
}
#fontTop{
 width: 70px;
 height: 70px;
 margin: 0px auto;
 border-radius: 50%;
 margin-top: -35px;
 background-color:#eff3f4;
 color:#3bbfe6;
 opacity: 0.8;
 filter: Alpha(Opacitydd = 30);
 transition:0.5s;
 float: left;
margin-left: 41%;
}
#fontTop span{
margin: 0px auto;
line-height: 101px;
width: 35px;
display: block;
font-size: 16px;
}
#fontAdjustContainer{
background-color: #fff;
width: 50px;
height: 136px;
border-radius: 0px 0px 30px 30px;
opacity: 0.7;
filter: Alpha(Opacitydd = 30);
border-bottom: solid 2px #ccc;
transition: 0.5s;
margin-right: 80px;
float: right;
margin-top: -138px;
border-right: solid 1px #e3e3e3;
border-left: solid 1px #e3e3e3;
position: relative;
}
#sizeAdd{
width: 40px;
height: 40px;
background: #909193;
border-radius: 50%;
margin: 0px auto;
margin-top: 10px;
}
#sizeDecrease{
	width: 40px;
height: 40px;
background: #909193;
border-radius: 50%;
margin: 0px auto;
margin-top: 6px;
}
.horizontal{
width: 34px;
height: 8px;
background-color: #fff;
position: relative;
top: 16px;
margin: 0px auto;
}
.vertical{
width: 8px;
height: 34px;
background-color: #fff;
position: relative;
top: -4px;
margin: 0px auto;
}