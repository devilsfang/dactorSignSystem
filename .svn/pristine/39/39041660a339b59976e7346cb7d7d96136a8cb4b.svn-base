/* -----------------------------头部样式-------------*/
#scsContacts_cxc{
	background-color: #dde5e8;
}
#patientShow {
	background-color: white;
	width: 100%;
	height: 60px;
	padding-top: 20px;
}

#patientLeft {
	float: left;
	padding-left: 4px;
}

#patientRight {
	float: right;
}

#patientImg {
	width: 40px;
	height: 40px;
	float: left;
}

#patientImg img {
	width: 40px;
	border-radius: 50%;
	margin-top: 8px;
	margin-left: 7px;
}

#patientInfo {
	float: left;
}

#doctorInfo {
	float: left;
}

#doctorCall {
	float: left;
	margin-top: 10px;
	position: relative;
	left: -20px;
}

#patientName {
	font-size: 15px;
	margin-top: 10px;
	margin-left: 15px;
}

#patientNumber {
	margin-left: 15px;
