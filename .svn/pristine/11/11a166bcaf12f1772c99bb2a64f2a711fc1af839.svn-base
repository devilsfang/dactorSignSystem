.l-page-header {
	height: 52px;
	overflow: hidden;
	display:block;
	background-color: #FFF;
	position: relative;

}

.l-page-header:before{
	content: " ";
    width: 200%;
    height: 200%;
    position: absolute;
    top: 0;
    left: 0;
    border-bottom: 1px solid #ccc;
    -webkit-transform: scale(.5);
    transform: scale(.5);
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
    box-sizing: border-box;
}

.l-page-header-title {
	

	font-size: 16px;
	line-height: 16px;
	text-align: center;
	width: 100%;
	letter-spacing: 1px;
