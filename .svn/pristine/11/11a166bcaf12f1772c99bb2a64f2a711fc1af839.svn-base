	position: relative;
	left: 50%;
}
    .messageBox {
    	display: none;
    	position: fixed;
    	top: 50%;
    	left: 50%;
    	transform: translate(-50%,-50%);
    	z-index: 999;
    	padding: 1em 2em;
    	border-radius: 10px;
    	text-align: center;
    	font-size: 14px;	
